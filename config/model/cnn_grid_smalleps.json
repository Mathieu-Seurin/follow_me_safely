{
  "name": "dqn_conv_grid_smaller_eps",

  "agent_type" : "dqn",
  "discount_factor": 0.99,

  "dqn_params": {

    "model_type" : "conv",

    "model_params": {
      "n_layers" : 3,
      "out_channels" : [16,32,64],
      "kernel_size" : [2,2,2],
      "pooling" : [2],

      "n_mlp_hidden" : 64,
      "use_memory" : false

    },

    "feedback_percentage_in_buffer" : 0,
    "memory_size": 40000,

    "clamp_grad": true,
    "optimizer": "rmsprop",
    "learning_rate": 5e-4,
    "weight_decay" : 0,

    "batch_size": 128,

    "exploration_method": {
      "name" : "eps_greedy",
      "begin_eps" : 1,
      "expected_step_explo" : 10000,
      "epsilon_minimum" : 0.04

    },

    "biased_sampling" : false,

    "update_every_n_ep" : 40,

    "apply_loss_to_target" : false,

    "consistency_loss_weight" : 0,

    "classification_margin" : 0,

    "classification_loss_weight" : 0,
    "classification_max_loss_weight" : 0,

    "percent_classification_margin" : 0,
    "classification_percent_loss_weight" : 0

  }
}