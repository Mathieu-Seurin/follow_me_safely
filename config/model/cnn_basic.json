{
  "name": "dqn_conv_agent_update_4",

  "agent_type" : "dqn",
  "discount_factor": 0.999,

  "biased_sampling" : true,

  "dqn_params": {

    "model_type" : "conv",

    "model_params": {
      "n_layers" : 2,
      "out_channels" : [8,16],
