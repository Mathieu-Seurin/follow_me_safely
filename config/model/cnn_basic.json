{
  "name": "dqn_conv_agent_prio",

  "agent_type" : "dqn",
  "discount_factor": 0.99,

  "dqn_params": {

    "model_type" : "conv",

    "model_params": {
      "n_layers" : 3,
      "out_channels" : [16,32,64],
      "kernel_size" : [2,2,2],
      "pooling" : [2],

      "n_mlp_hidden" : 64,
      "use_memory" : false

    },

    "memory" : "prioritized",
    "memory_size": 50000,

    "clamp_grad": true,
    "optimizer": "rmsprop",
    "learning_rate": 5e-4,
    "batch_size": 128,

    "exploration_method": {
      "name" : "eps_greedy",
      "begin_eps" : 1,
      "expected_step_explo" : 30000,
      "epsilon_minimum" : 0.04

    },

    "update_every_n_ep" : 40
  }
}