    "memory" : "simple",
    "memory_size": 40000,

    "clamp_grad": true,
    "optimizer": "rmsprop",
    "learning_rate": 5e-4,
    "weight_decay" : 0,

    "batch_size": 128,

    "exploration_method": {
      "name" : "eps_greedy",
      "begin_eps" : 1,
      "expected_step_explo" : 30000,
      "epsilon_minimum" : 0.04

    },

    "update_every_n_ep" : 4,

    "use_consistency_loss" : false,
    "consistency_loss_weight" : 1,

    "use_margin_loss" : false,
    "classification_margin" : 0,
    "classification_loss_weight" : 0

  }
