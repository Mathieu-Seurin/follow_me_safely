{
  "name" : "impala_cnn_no_lstm",
  "algo":"IMPALA",

  "algo_config":
  {
    "num_envs_per_worker" : 6,
    "num_workers" : 24,
    "num_gpus": 1,

    "lr_schedule": [
      [0, 0.0005],
      [20000000, 0.000000000001]
    ],

    "clip_rewards": true,
    "train_batch_size" : 1000,
    "sample_batch_size" : 50,

    "model": {
      "custom_model": "conv_policy",
      "custom_preprocessor": null,

      "use_lstm" : false,
      "lstm_cell_size": 128,
      "lstm_use_prev_action_reward": true,

      "custom_options" : {

        "vision" : {

          "stem_config" :{
            "n_layers" : 3,
            "n_channels" : [16,32,64],
            "kernel" : [[2,2],[2,2],[2,2]],
            "pooling" : [[2,2]],
            "stride" : [[1,1],[1,1],[1,1]]
          }

        }

      }
    }
  }
}

